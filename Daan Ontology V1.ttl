###  http://www.semanticweb.org/mdebe/ontologies/NGO#UIProfile
ngo:UIProfile rdf:type owl:Class ;
              rdfs:subClassOf prov:Location ;
              dcterms:description "A profile that describes the preferences for the types of Agents that a Funder prefers to fund" ;
              rdfs:comment """# SPARQL query based on profile:
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX fti: <http://franz.com/ns/allegrograph/2.2/textindex/>
PREFIX ngo: <http://www.semanticweb.org/mdebe/ontologies/NGO#>
PREFIX sdg: <http://www.semanticweb.org/mdebe/ontologies/2022/10/UNSDG#>

SELECT ?ngo 
WHERE {ngo:CurrentKGState ngo:kgStateProfile ?profile.
       ?profile ngo:locationPreference ?location;
	            ngo:sdgPreference ?sdg.
		?ngo    ngo:hasLocation ?ngoloc;
		        ngo:relatesToSDG ?sdg.
		OPTIONAL{?ngoloc ngo:isContainedIn ?location.}
		OPTIONAL{?profile ngo:minimumAnualBudget ?pminbudg.
		         ?ngo ngo:hasAnnualBudget ?ngobudg.
		         BIND(IF(?ngobudg < ?pminbudg, \"False\", \"True\") as ?mintest)} 
		OPTIONAL{?profile ngo:maximumAnualBudget ?pmaxbudg.
		         ?ngo ngo:hasAnnualBudget ?ngobudg.
		         BIND(IF(?ngobudg > ?pmaxbudg, \"False\", \"True\") as ?maxtest)}
		         BIND(BOUND(?ngoloc) AS ?locmatch)
				 BIND(IF(BOUND(?mintest), ?mintest, \"True\") AS ?maxmatch)
				 BIND(IF(BOUND(?mintest), ?mintest, \"True\") AS ?minmatch)
		FILTER(?minmatch = \"True\")
		FILTER(?maxmatch = \"True\")
		FILTER(BOUND(?locmatch) || ?ngoloc = ?location)}""" ;
              rdfs:label "Funder Profile" .


